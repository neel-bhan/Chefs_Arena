            <children>
            
                <Label fx:id="welcomeText" />
               <ImageView fitHeight="739.0" fitWidth="1293.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@images/chefs_arena_background.png" />
                  </image>
               </ImageView>
                <Button onAction="#playgame" prefHeight="50.0" prefWidth="650.0" text="Start">
                  <StackPane.margin>
                     <Insets bottom="140.0" top="528.0" />
                  </StackPane.margin>
               </Button>
            </children>
         </StackPane>
      </VBox>
   </children>
</AnchorPane>
